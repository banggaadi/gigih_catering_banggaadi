<div class="menu-container">
    <div class="row">
        <% @products.each do |product| %>
        <div class="col-4">
            <h5> <%= product.name%> </h5>
            <p> <%= product.description %> </p>
            <p> <%= product.category %> </p>
            <span> <%= product.price %> </span>
        </div>
        <% end %>
    </div>
</div>
